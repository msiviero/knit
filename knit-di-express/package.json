{
  "name": "@msiviero/knit-di-express",
  "version": "0.0.3",
  "main": "dist/index.js",
  "license": "MIT",
  "keywords": [
    "nodejs",
    "dependency injection",
    "decorators",
    "typescript",
    "express"
  ],
  "description": "Knit dependency injection bindings for express",
  "scripts": {
    "compile": "npm run-script clean && tsc -p ./tsconfig.build.json",
    "test": "jest --detectOpenHandles",
    "lint": "tslint -c ../tslint.json -p tsconfig.json",
    "clean": "rimraf dist",
    "prepare": "npm run-script lint && npm run-script compile"
  },
  "jest": {
    "globals": {
      "tsConfigFile": "tsconfig.json"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/test/**/*.test.(ts)"
    ],
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/jest": "^23.1.4",
    "@types/supertest": "^2.0.4",
    "jest": "^23.3.0",
    "supertest": "^3.1.0",
    "ts-jest": "^23.0.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "@msiviero/knit-di": "^0.0.2",
    "express": "^4.16.3",
    "reflect-metadata": "^0.1.12"
  }
}